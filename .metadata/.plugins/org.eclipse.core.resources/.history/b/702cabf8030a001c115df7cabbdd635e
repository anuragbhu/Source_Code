import java.util.Scanner;

public class ReverseArray {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int[] arr = new int[n];
		for(int i = 0; i <n ; i++) 
			arr[i] = sc.nextInt();
		int start = 0;
		
		reverseArrayIterative(arr, n);  // TC = O(n), SC = O(1)
//		reverseArrayRecursive(arr, start, n-1);  // TC = O(n), SC = O(1)
		printArray(arr, n);
		sc.close();

	}
	
	public static void reverseArrayRecursive(int[] arr, int start, int end) {

		if(start >= end)
			return;
		
		int temp = arr[start];
		arr[start] = arr[end];
		arr[end] = temp;
		reverseArrayRecursive(arr, start+1, end-1);
	}
	
	public static void reverseArrayIterative(int[] arr, int n) {
		
		int i = -1, j = n;
		while(++i < --j) {
			
//			int temp = arr[i];
//			arr[i] = arr[j];
//			arr[j] = temp;
			
			arr[i] += arr[j];
			arr[j] = arr[i] - arr[j];
			arr[i] -= arr[j];
			
//			i++;
//			j--;
		}
	}
	
	public static void printArray(int[] arr, int n) {
		for(int i = 0; i < n; i++)
			System.out.print(arr[i] + " ");
	}
		
}
